{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "Object for saving correction data associated with LabTools",
    "type": "object",
    "name": "Test",
    "properties": {
        "ltid": {
            "type": "string"
        },
        "athlete": {
            "type": "string"
        },
        "analysis": {
            "type": "string"
        },
        "correction": {
            "type": "string"
        },

        "original_filenames": {
            "type": "object",
            "properties": {
                "sprint": {
                    "type": "object",
                    "properties": {
                        "c2d": {
                            "type": "string"
                        }
                    }
                },
                "ramp": {
                    "type": "object",
                    "properties": {
                        "c2d": {
                            "type": "string"
                        },
                        "xml": {
                            "type": "string"
                        }
                    }
                },
                "trough": {
                    "type": "object",
                    "properties": {
                        "c2d": {
                            "type": "string"
                        }
                    }
                }
            }
        },

        "weight": {
            "type": "number"
        },
        "height": {
            "type": "number"
        },
        "skinfold_thickness": {
            "type": "object",
            "properties": {
                "chest": {
                    "type": "number"
                },
                "stomach": {
                    "type": "number"
                },
                "biceps": {
                    "type": "number"
                },
                "hips": {
                    "type": "number"
                },
                "thigh": {
                    "type": "number"
                },
                "shoulderblade": {
                    "type": "number"
                },
                "triceps": {
                    "type": "number"
                },
                "lower_back": {
                    "type": "number"
                },
                "calf": {
                    "type": "number"
                }
            }
        },

        "location": {
            "type": "string"
        },
        "temperature": {
            "type": "string"
        },
        "date": {
            "type": "string"
        },
        "ergometer_serial_nr": {
            "type": "string"
        },
        "testing_bicycle": {
            "type": "string"
        },
        "gear_ratio": {
            "type": "string"
        },

        "sprint": {
            "type": "object",
            "properties": {
                "lactate": {
                    "type": "object",
                    "properties": {
                        "vbla1": {
                            "type": "number"
                        },
                        "vbla2": {
                            "type": "number"
                        },
                        "vbla3": {
                            "type": "number"
                        },
                        "nblamax": {
                            "type": "number"
                        }
                    }
                },
                "alactazide": {
                    "type": "object",
                    "properties": {
                        "time": {
                            "type": "number"
                        },
                        "wattage": {
                            "type": "number"
                        }
                    }
                },
                "peak": {
                    "type": "object",
                    "properties": {
                        "time": {
                            "type": "number"
                        },
                        "wattage": {
                            "type": "number"
                        }
                    }
                },
                "max_force": {
                    "type": "number"
                },
                "end_wattage": {
                    "type": "number"
                },
                "avg_wattage": {
                    "type": "number"
                }
            }
        },

        "ramp": {
            "type": "object",
            "properties": {
                "vo2": {
                    "type": "object",
                    "properties": {
                        "start": {
                            "type": "number"
                        },
                        "end": {
                            "type": "number"
                        },
                        "excluded": {
                            "type": "array",
                            "items": {
                                "type": "number"
                            }
                        },
                        "average": {
                            "type": "number"
                        }
                    }
                },
                "max_heartfrequency": {
                    "type": "number"
                },
                "max_wattage": {
                    "type": "number"
                }
            }
        },

        "trough": {
            "type": "object",
            "properties": {
                "starting_wattage": {
                    "type": "number"
                },
                "starting_wattage_epsilon": {
                    "type": "number"
                },
                "lactate": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                },
                "IANS": {
                    "type": "object",
                    "properties": {
                        "heartfrequency": {
                            "type": "object",
                            "properties": {
                                "value": {
                                    "type": "number"
                                },
                                "wattage": {
                                    "type": "number"
                                }
                            }
                        },
                        "wattage": {
                            "type": "object",
                            "properties": {
                                "value": {
                                    "type": "number"
                                },
                                "wattage": {
                                    "type": "number"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}